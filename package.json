{
  "name": "cli-profile-manager",
  "version": "0.1.0",
  "description": "Save, share, and load CLI profiles - a marketplace for Claude Code and GitHub CLI configurations",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "cpm": "dist/cli.js",
    "claude-profiles": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "test": "npm run build && node --test dist/test/*.test.js"
  },
  "keywords": [
    "claude",
    "anthropic",
    "github",
    "copilot",
    "cli",
    "profiles",
    "configuration",
    "dotfiles"
  ],
  "author": "brrichards",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/brrichards/cli-profile-manager.git"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "inquirer": "^9.2.12",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.0",
    "typescript": "^5.3.3"
  }
}
